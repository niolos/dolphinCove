<div class="program">
    <h1>Programs</h1>
    <table>
        <tr>
            <th>Program Name</th>
            <th>Number of Guests</th>
            <th>Cost</th>
            <th>Description</th>
            <th colspan="2">Action</th>
        </tr>
        <% info.forEach(function(data) {%>
                <tr>
                    <td><%= data.p_name%></td>
                    <td><%= data.num_of_guest%></td>
                    <td>$<%= data.cost%></td>
                    <td><%= data.description%></td>
                    <td>
                        <a href="/editPro/<%=data.id%>">
                            <i class="fa-solid fa-pen-to-square"></i>
                        </a>
                        <a href="editPro/delete/<%=data.id%>">
                            <i class="fa-solid fa-trash-can"></i>
                        </a>
                    </td>
                </tr>
        <% }); %>
    </table>
</div>