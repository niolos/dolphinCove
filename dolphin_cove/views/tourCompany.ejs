<div class="tourComp">
    <h1>Tour Companies</h1>
    <table>
        <tr>
            <th>Company Name</th>
            <th>Email</th>
            <th>Password</th>
            <th colspan="2">Action</th>
        </tr>
        <% info.forEach(function(data) {%>
                <tr>
                    <td><%= data.comp_name%></td>
                    <td><%= data.email%></td>
                    <td><%= data.password%></td>
                    <td><input type="hidden" value="<%data.id%>"></td>
                    <td>
                        <a href="/editTour/<%=data.id%>">
                            <i class="fa-solid fa-pen-to-square"></i>
                        </a>
                        <a href="editTour/delete/<%=data.id%>">
                            <i class="fa-solid fa-trash-can"></i>
                        </a>
                    </td>
                </tr>
        <% }); %>
    </table>
</div>